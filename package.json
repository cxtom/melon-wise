{
  "name": "melon-wise",
  "version": "0.0.17",
  "description": "ui components for react on mobile",
  "main": "./src/index.js",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-plugin-external-helpers": "^6.5.0",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-react-jsx": "^6.5.2",
    "babel-plugin-transform-strict-mode": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "expect": "^1.14.0",
    "expect-jsx": "^2.3.0",
    "isparta": "^4.0.0",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "nib": "^1.1.0",
    "react-addons-test-utils": "^0.14.7",
    "stylus": "^0.53.0"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha test/components/** --compilers js:babel-core/register --require ./test/setup.js",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report html _mocha -- test/components/** --require ./test/setup.js",
    "lint": "fecs src --rule",
    "build:amd": "sh tool/build-amd.sh",
    "build:commonjs": "sh tool/build-commonjs.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxtom/melon-wise.git"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-tap-event-plugin": ">=0.1.3"
  },
  "keywords": [
    "react",
    "ui",
    "components"
  ],
  "author": "cxtom2010@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cxtom/melon-wise/issues"
  },
  "homepage": "https://github.com/cxtom/melon-wise#readme",
  "edp": {
    "wwwroot": "/",
    "depDir": "dep",
    "srcDir": "src",
    "loaderAutoConfig": "js,htm,html,tpl,vm,phtml",
    "loaderUrl": "http://s1.bdstatic.com/r/www/cache/ecom/esl/1-8-2/esl.js",
    "dependencies": {}
  },
  "dependencies": {
    "melon-classname": "^0.1.6",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
}
