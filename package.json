{
  "name": "melon-wise",
  "version": "0.0.16",
  "description": "ui components for react on mobile",
  "main": "./src",
  "directories": {
    "doc": "doc",
    "test": "test",
    "example": "example"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "edp-provider-rider": "^2.0.0",
    "expect": "^1.13.0",
    "expect-jsx": "^2.1.2",
